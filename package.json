{
  "name": "@yafh/vue-logger",
  "version": "0.0.1",
  "description": "Logger utility for Vue.js 3",
  "type": "module",
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "vue",
    "vue.js",
    "vue3",
    "logger",
    "log",
    "console"
  ],
  "author": "YanAndFish",
  "license": "MIT",
  "homepage": "https://github.com/YanAndFish/vue-logger#readme",
  "bugs": {
    "url": "https://github.com/YanAndFish/vue-logger/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main":"./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3.2",
    "vue": "^3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vue/reactivity": "^3.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vue": "^3.2"
  },
  "dependencies": {
    "chalk": "^5.2.0"
  }
}
